{
  "version": "2.0",
  "extensions": {
    "eventHubs": {
      "maxEventBatchSize": 10,
      "batchCheckpointFrequency": 1,
      "prefetchCount": 300,
      "transportType": "amqpWebSockets",
      "clientRetryOptions": {
        "mode": "exponential",
        "tryTimeout": "00:01:00",
        "delay": "00:00:00.80",
        "maximumDelay": "00:01:00",
        "maximumRetries": 3
      }
    }
  },
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    }
  }
}